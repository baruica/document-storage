{
    "name": "baruica/document-storage",
    "type": "library",
    "description": "Provides implementations to interact with various document storage services",
    "keywords": ["document-storage"],
    "homepage": "https://github.com/baruica/document-storage",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": { "DocumentStorage\\": "src" },
